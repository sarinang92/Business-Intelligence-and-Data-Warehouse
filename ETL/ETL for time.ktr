    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>// ===== Helpers =====
function dateOf(y, m, d) { // m = 1..12
  var c = java.util.Calendar.getInstance();
  c.set(y, m - 1, d, 0, 0, 0);
  c.set(java.util.Calendar.MILLISECOND, 0);
  return c.getTime();
}
function addDays(date, days) {
  var c = java.util.Calendar.getInstance();
  c.setTime(date);
  c.add(java.util.Calendar.DAY_OF_MONTH, days);
  return c.getTime();
}
var fmtYMD = new java.text.SimpleDateFormat("yyyy-MM-dd");
function ymd(date) { return fmtYMD.format(date); }

// Easter Sunday (Meeus/Jones/Butcher)
function easterSunday(year) {
  var a = year % 19;
  var b = Math.floor(year / 100);
  var c = year % 100;
  var d = Math.floor(b / 4);
  var e = b % 4;
  var f = Math.floor((b + 8) / 25);
  var g = Math.floor((b - f + 1) / 3);
  var h = (19*a + b - d - g + 15) % 30;
  var i = Math.floor(c / 4);
  var k = c % 4;
  var l = (32 + 2*e + 2*i - h - k) % 7;
  var m = Math.floor((a + 11*h + 22*l) / 451);
  var month = Math.floor((h + l - 7*m + 114) / 31);   // 3=March, 4=April
  var day = ((h + l - 7*m + 114) % 31) + 1;
  return dateOf(year, month, day);
}

// ===== Options (toggle as needed) =====
var includeCarnival = false;      // Carnival Mon/Tue are NOT federal holidays
var includeCorpusChristi = false; // Corpus Christi is generally ponto facultativo
var markWeekendsAsHoliday = false;

// ===== Build holiday set for the row's year =====
var cal = java.util.Calendar.getInstance();
cal.setTime(date);
var year = cal.get(java.util.Calendar.YEAR);

var holidays = new java.util.LinkedHashMap(); // key "yyyy-MM-dd" -> name
function addHoliday(date, name) { holidays.put(ymd(date), name); }

var easter = easterSunday(year);

// --- National (federal) holidays ---
addHoliday(dateOf(year, 1, 1),   "Confraternização Universal (New Year's Day)");
addHoliday(addDays(easter, -2),  "Sexta-feira Santa (Good Friday)");
addHoliday(dateOf(year, 4, 21),  "Tiradentes");
addHoliday(dateOf(year, 5, 1),   "Dia do Trabalhador (Labour Day)");
addHoliday(dateOf(year, 9, 7),   "Independência do Brasil");
addHoliday(dateOf(year,10,12),   "Nossa Senhora Aparecida");
addHoliday(dateOf(year,11, 2),   "Finados (All Souls' Day)");
addHoliday(dateOf(year,11,15),   "Proclamação da República");
// Black Consciousness Day became national from 2024 onward
if (year >= 2024) {
  addHoliday(dateOf(year,11,20), "Dia da Consciência Negra");
}
addHoliday(dateOf(year,12,25),   "Natal (Christmas)");

// --- Optional nationwide observances (toggle above) ---
if (includeCarnival) {
  addHoliday(addDays(easter, -48), "Carnaval (Segunda-feira)"); // Monday
  addHoliday(addDays(easter, -47), "Carnaval (Terça-feira)");   // Tuesday
  // Ash Wednesday morning (half day) commonly observed; omit or add as desired
  // addHoliday(addDays(easter, -46), "Quarta-feira de Cinzas (até 14h)");
}
if (includeCorpusChristi) {
  addHoliday(addDays(easter, 60),  "Corpus Christi");
}

// ===== Result for this row =====
var key = ymd(date);
var holiday_name = holidays.get(key);
var is_holiday = (holiday_name !== null);

// Optional: treat weekends as holidays too
if (!is_holiday &amp;&amp; markWeekendsAsHoliday) {
  var dow = cal.get(java.util.Calendar.DAY_OF_WEEK); // 1=Sun..7=Sat
  if (dow === java.util.Calendar.SATURDAY || dow === java.util.Calendar.SUNDAY) {
    is_holiday = true;
    holiday_name = (dow === java.util.Calendar.SATURDAY) ? "Sábado" : "Domingo";
  }
}

// Expose outputs
is_holiday = is_holiday;   // Boolean</jsScript_script>
      </jsScript>
    </jsScripts>
