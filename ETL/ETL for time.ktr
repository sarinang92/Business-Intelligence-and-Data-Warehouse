        <jsScript_script>var start = new Date("2016-01-01");
var end   = new Date("2020-12-31");

var current = start;
while (current &lt;= end) {
  var row = createRowCopy(getOutputRowMeta().size());
  row[0] = new java.sql.Date(current.getTime()); // output as date
  putRow(row);
  current.setDate(current.getDate() + 1); // increment 1 day
}
</jsScript_script>
